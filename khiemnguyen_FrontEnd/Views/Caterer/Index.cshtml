		<section id="menupage-7-menu-section">
			<div class="container ptb-100 line">
				<span class="line-3"></span>
				<span class="line-4"></span>
				<span class="line-5"> </span>
				<div class="homepage1-menu-top-sec wow fadeInUp" data-wow-duration="1s" data-wow-delay="3s">
					<p class="homemenu-txt1"><span class="pr-10"><img src="~/images/Homepage1/yellow-star.png" alt="yellow-star"></span>Our Menu<span class="pf-10"><img src="~/images/Homepage1/yellow-star.png" alt="yellow-star"></span></p>
				@* 	<h2 class="homemenu-txt2">FOOD MENU</h2> *@
@* 					<p class="homemenu-txt3">Diam leo massa pellentesque a neque turpis cum mi gravida. Amet massa adipiscing mi dictum urna commodo. Fringilla ipsum etiam habitasse dolor lacus viverra.</p>
 *@				</div>
			
		
				<div class="homepage1-menu-bottom-sec wow fadeInUp" data-wow-duration="1.8s" data-wow-delay="3s">
					<h3 class="d-none">Menu List</h3>
				
				
					<div class="tab-content">
						<div class="align-items-start">
							<a class="btn btn-warning" asp-action="CreateMenu" asp-route-Cid="@ViewBag.Cid">Create a New Menu</a>
						</div>
						<div class="tab-pane active" id="allmenu" role="tabpanel">
							
							<br />
							<br />
							<div class="tab-description menupage7-menu">
						
								@foreach(var obj in Model){
									@if (APIControl.Role!="Admin111")
									{
										<a asp-action="EditMenu" asp-route-id="@obj.id">
											<div class="menupage-7-menu-content">
												<div class="menupage7-menu-top">
													@if (obj.Image == null)
													{
														<img class="card-img-top"
						 style="height: 320px"
						 alt="profile picture" src="~/images/menupage-7/menu-1.png" alt="menu-item-img">
													}
													else
													{
														<img class="card-img-top"
															 style="height: 320px"
															 alt="profile picture" src="data:image/jpg;base64,@Convert.ToBase64String(obj.Image)">

																			}
												</div>
												<div class="menupage7-menu-bottom">
													<div class="menupage7-deatils">
														<span class="menu7-txt1">@obj.Name</span>
														<span class="menu7-txt2">$@obj.Price.ToString("###.00")</span>
													</div>
													<p class="menu7-txt3">@obj.Description</p>
													<div>
														@for (int x = 1; x <= obj.israted; x++)
														{
															<span><img src="~/images/single-shop/star.svg" alt="star-img"></span>
														}
													</div>
												</div>

											</div>
										</a>
													}else
										{
										<a asp-action="EditMenu" asp-route-id="@obj.id">
											<div class="menupage-7-menu-content">
												<div class="menupage7-menu-top">
													@if (obj.Image == null)
													{
														<img class="card-img-top"
						 style="height: 320px"
						 alt="profile picture" src="~/images/menupage-7/menu-1.png" alt="menu-item-img">
													}
													else
													{
														<img class="card-img-top"
															 style="height: 320px"
															 alt="profile picture" src="data:image/jpg;base64,@Convert.ToBase64String(obj.Image)">

													}
												</div>
												<div class="menupage7-menu-bottom">
													<div class="menupage7-deatils">
														<span class="menu7-txt1">@obj.Name</span>
														<span class="menu7-txt2">$@obj.Price.ToString("###.00")</span>
													</div>
													<p class="menu7-txt3">@obj.Description</p>
													<div>
														@for (int x = 1; x <= obj.israted; x++)
														{
															<span><img src="~/images/single-shop/star.svg" alt="star-img"></span>
														}
													</div>
												</div>

											</div>
										</a>
											
										}

								}
								
								
								
								
								
